{
    "name": "Youtube Options",
    "version": "1.0",
    "description": "Youtube Options",
    "background": {
        "scripts": ["background.js"],
        "persistent": false
    },
    "browser_action": {
        "default_icon": "images/iconActive.png"
    },
    "permissions": [
        "webNavigation",
        "https://www.youtube.com/*",
        "tabs"
    ],
    "manifest_version": 2
}