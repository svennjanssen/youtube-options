{
    "name": "Youtube Options",
    "version": "1.0",
    "description": "Youtube Options",
    "background": {
        "scripts": ["background.js"],
        "persistent": false
    },
    "browser_action": {
        "default_popup": "popup.html"
    },
    "permissions": [
        "webNavigation",
        "https://www.youtube.com/*",
        "tabs"
    ],
    "manifest_version": 2
}